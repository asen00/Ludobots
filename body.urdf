<robot name = "robot">
    <link name="Link1">
        <inertial>
            <origin xyz="0 0 1.25" rpy="0 0 0"/>
            <mass value="1" />
            <inertia ixx="100" ixy="0" ixz="0" iyy="100" iyz="0" izz="100" />
        </inertial>
        <visual>
            <origin xyz="0 0 1.25" rpy="0 0 0"/>
            <geometry>
                <box size="3 2 0.5" />
            </geometry>
            <material name="Cyan">
                <color rgba="0 1.0 1.0 1.0"/>
            </material>
        </visual>
        <collision>
            <origin xyz="0 0 1.25" rpy="0 0 0"/>
            <geometry>
                <box size="3 2 0.5" />
            </geometry>
        </collision>
    </link>
    <joint name="Link1_Wheel1" type="continuous">
       <parent link="Link1"/>
       <child  link="Wheel1"/>
       <origin rpy="0 0 0" xyz="-0.75 0 1" />
     <axis xyz="0 1 0"/>
       <limit effort="1.0" lower="-3.14159" upper="3.14159" velocity="0.0"/>
    </joint>
